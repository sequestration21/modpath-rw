# Define the directories for the object and module files,
# the executable, and the executable name and path.
OBJDIR = ./obj_temp
BINDIR = .
PROGRAM = mpathrwif

SOURCEDIR1=../source
SOURCEDIR2=../lib/gpkde/src
SOURCEDIR3=../lib/finterp/src
SOURCEDIR4=../lib/rngzig/

VPATH = \
${SOURCEDIR1} \
${SOURCEDIR2} \
${SOURCEDIR3} \
${SOURCEDIR4} 

.SUFFIXES: .c .cpp .f .f90 .F90 .fpp .o

# Define the Fortran compile flags
FC = ifort
# FROM MODPATH-OMP
FFLAGS = -Ofast -traceback -qopenmp -ipo
# DEBUG
#FFLAGS = -O0 -g -debug extended -traceback -qopenmp


# Define the C compile flags
CC = gcc
CFLAGS = -O3 -D_UF 

# Define the libraries
SYSLIBS = 

# Start with objects from GPKDE, 
# then the library for linear interpolation, 
